<web-app>
	<display-name>Archetype Created Web Application</display-name>
	<!--  <servlet>
	  <servlet-name>ServletProject</servlet-name>
	  <servlet-class>com.cnts.EmployeeServlet</servlet-class>
 </servlet>
 <servlet-mapping>
	  <servlet-name>ServletProject</servlet-name>
