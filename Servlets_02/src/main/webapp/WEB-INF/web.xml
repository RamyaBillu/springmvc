 </servlet-mapping>-->
	<servlet>
		<servlet-name>WorkerServlet</servlet-name>


		<servlet-class>com.cnts.WorkerServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>WorkerServlet</servlet-name>
		<url-pattern>/sp</url-pattern>
	</servlet-mapping>

	<servlet>
		<init-param>
			<param-name>company</param-name>
			<param-value>corenuts Technology</param-value>
		</init-param>
		<init-param>
			<param-name>password</param-name>
			<param-value>core@23345</param-value>
		</init-param>
		<servlet-name>CookieStore</servlet-name>
		<servlet-class>com.cnts.CookieStore</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>CookieStore</servlet-name>
		<url-pattern>/ck</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>SessionClass</servlet-name>
		<servlet-class>com.cnts2.SessionClass</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>SessionClass</servlet-name>
		<url-pattern>/ss</url-pattern>
	</servlet-mapping>
	<context-param>
		<param-name>username</param-name>
		<param-value>pandu</param-value>
	</context-param>

	<context-param>
		<param-name>password</param-name>
		<param-value>pandu@234</param-value>
	</context-param>
	<filter>
		<filter-name>FilterOutput</filter-name>
		<filter-class>com.cnts2.FilterOutput</filter-class>
	</filter>
	<filter-mapping>
		<filter-name>FilterOutput</filter-name>
		<url-pattern>/ft</url-pattern>
	</filter-mapping>
	<listener>
		<listener-class>com.cnts2.MyContextListner</listener-class>
	</listener>
	<listener>
		<listener-class>com.cnts2.MySessionListner</listener-class>
	</listener>


</web-app>